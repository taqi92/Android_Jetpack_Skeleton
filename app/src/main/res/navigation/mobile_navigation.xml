<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="co.basic.androidjetpackskeleton.ui.topRated.TopRatedFragment"
        android:label="@string/title_new_release"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="co.basic.androidjetpackskeleton.ui.newRelease.NewReleaseFragment"
        android:label="@string/title_top_rated"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="co.basic.androidjetpackskeleton.ui.upcoming.UpcomingFragment"
        android:label="@string/title_upcoming"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_favourites"
        android:name="co.basic.androidjetpackskeleton.ui.favourites.FavouritesFragment"
        android:label="@string/title_favourites"
        tools:layout="@layout/fragment_favourites" />


</navigation>